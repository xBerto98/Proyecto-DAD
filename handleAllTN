	private void handleAllTN(RoutingContext routingContext) {
		mySQLClient.getConnection(connection -> {
			if(connection.succeeded()) {
				connection.result().query("SELECT cont, acierto FROM dad.tecladosnumericos ORDER BY idTN DESC LIMIT 1;", result -> {
					if(result.succeeded()) {
						String jsonResult=result.result().toJson().encodePrettily();
						Integer cont = result.result().getResults().get(0).getInteger(0);
						Integer acierto = result.result().getResults().get(0).getInteger(1);
						if(cont==3 && acierto==0) {
							System.out.println("No puedes meter contrase√±as en 10 minutos");
						} else {
							System.out.println("Adelante muchacho");
						}
						routingContext.response().end(jsonResult);
					} else {
						System.out.println(result.cause().getMessage());
						routingContext.response().setStatusCode(400).end();
					}
					connection.result().close();
				});
			} else {
				routingContext.response().end();
				connection.result().close();
				System.out.println(connection.cause().getMessage());
				routingContext.response().setStatusCode(400).end();
			}
		});
	}
